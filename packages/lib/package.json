  ],
  "scripts": {
    "start-setup": "node --experimental-specifier-resolution=node --loader ts-node/esm $(grep SETUP_ENTRY .package.paths | cut -d '=' -f2)",
    "coverage": "npm test -- --coverage",
    "cp-files": "./scripts/cp-files.sh",
    "lib-clean": "./scripts/lib-clean.sh",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "docs": "documentation readme src --section=API",
    "clean": "rm -fr dist/*",
    "prebuild": "npm run clean",
    "build": "npm run rollup",
    "decls": "tsc -p tsconfig.decls.json",
    "watch": "npm-watch",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublish-only": "npm run lint && npm test && npm run build",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "count": "./scripts/count-errors.sh",
    "test": "./scripts/test.py"
  },
  "dependencies": {
    "@babel/core": "^7.18.0",
    "@bitcoin-computer/bitcore-mnemonic-ltc": "8.25.41",
