{
  "name": "@bitcoin-computer/node-js-boilerplate",
  "version": "0.10.1",
  "description": "Minimalistic boilerplate to quick-start Bitcoin Computer development.",
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint --fix . --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@bitcoin-computer/lib": "^1.0.0",
    "bitcoin-computer-lib": "^0.8.0-beta",
    "eslint-plugin-prettier": "^4.0.0",
    "ts-node": "^10.8.1",
    "tslib": "~2.4.0"
  },
  "devDependencies": {
    "@types/jest": "~28.1.3",
    "@types/node": "~18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "~26.5.3",
    "jest": "~28.1.0",
    "prettier": "~2.7.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~28.0.3",
    "tsutils": "~3.21.0",
    "typescript": "~4.7.2"
  },
  "engines": {
    "node": ">= 16.13 <17"
  },
  "author": "Clemens Ley <clemens@bitcoincomputer.io>",
  "license": "MIT",
  "volta": {
    "node": "16.13.0"
  },
  "gitHead": "8d86e4e5094dca3ac15374fb16b56e4da6d1c2b6"
}
