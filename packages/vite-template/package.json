{
  "name": "@bitcoin-computer/vite-project",
  "version": "0.23.0-beta.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "build:turbo": "turbo run build",
    "clean": "npm run clean:logs",
    "clean:logs": "rm -f *.log 2> /dev/null",
    "deploy": "node --loader ts-node/esm  ./scripts/deploy.ts",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run",
    "types": "tsc --noEmit",
    "test:dev": "vitest",
    "test:show": "npm run test 2>&1 | tee vite-template-test.log && open vite-template-test.log"
  },
  "dependencies": {
    "@bitcoin-computer/components": "^0.23.0-beta.0",
    "@bitcoin-computer/lib": "^0.23.0-beta.0",
    "dotenv": "^16.4.5",
    "flowbite": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^20.11.20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.44",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  }
}
